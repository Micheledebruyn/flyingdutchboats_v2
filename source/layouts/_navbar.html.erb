<div class="navbar-dutch-boats">
  <div class="navbar-container">
    <% link_to "index.html" do %>
    <%= image_tag 'logo-text.png', alt: "Logo Flying Dutch Boats", class: "logo" %>
    <% end %>
    <div class="navbar-right-side hidden-xs">
      <div class="navbar-top">
        <div class="social-items">
          <% link_to dato.social_medium.facebook_link, target: "_blank" do %>
          <i class="fab fa-facebook fa-2x"></i>
          <% end %>

          <% link_to dato.social_medium.instagram_link, target: "_blank" do %>
          <i class="fab fa-instagram fa-2x"></i>
          <% end %>

          <% link_to dato.social_medium.tripadvisor_link, target: "_blank" do %>
          <i class="fab fa-tripadvisor fa-2x" aria-hidden="true"></i>
          <% end %>

        </div>
        <%= link_to dato.phone_number, "tel:#{ dato.phone_number }", class: "red-link" %>
        <%= link_to dato.navigation_bar.book_button, "booking.html", class: "btn-red"  %>
      </div>
      <div class="navbar-bottom">
        <%= link_to dato.navigation_bar.link_canal_cruises, "index.html" %>
        <%= link_to dato.navigation_bar.link_boat_rental, "boat_rental.html" %>
        <%= link_to dato.navigation_bar.link_faq, "faq.html" %>
        <%= link_to dato.navigation_bar.link_events, "events.html"  %>
        <%= link_to dato.navigation_bar.link_recommendations, "recommendations.html"  %>
        <%= link_to dato.navigation_bar.link_contact, "contact.html"  %>
      </div>
    </div>

    <button id="trigger-overlay" class="visible-xs" type="button">
      <i class="fas fa-bars"></i>
    </button>

    <div class="overlay overlay-contentscale">
      <button type="button" class="overlay-close">Close</button>
      <nav>
        <ul>
         <li>
           <%= link_to dato.navigation_bar.link_canal_cruises, "index.html", role: "menuitem", tabindex: "-1" %>
         </li>
         <li>
           <%= link_to dato.navigation_bar.link_boat_rental, "boat_rental.html", role: "menuitem", tabindex: "-1" %>
         </li>
         <li>
           <%= link_to dato.navigation_bar.link_faq, "faq.html", role: "menuitem", tabindex: "-1" %>
         </li>
         <li>
           <%= link_to dato.navigation_bar.link_events, "events.html" , role: "menuitem", tabindex: "-1" %>
         </li>
         <li>
           <%= link_to dato.navigation_bar.link_recommendations, "recommendations.html" , role: "menuitem", tabindex: "-1" %>
         </li>
         <li>
           <%= link_to link_to dato.navigation_bar.link_contact, "contact.html" , role: "menuitem", tabindex: "-1" %>
         </li>
         <li class="divider"></li>
         <li>
          <%= link_to link_to dato.navigation_bar.book_button, "booking.html", role: "menuitem", tabindex: "-1"  %>
        </li>
        <li>
          <%= link_to dato.navigation_bar.phone_number, "tel:+31647589217", role: "menuitem", tabindex: "-1" %>
        </li>
      </ul>
    </nav>
  </div>
</div>
</div>
